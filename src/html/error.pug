doctype html
html(lang="en")
  head
    title Walcron Coorperation
    include ../incl/meta.incl
  style.
    html {font-size: 100%;}
    body {
      font-size: 1rem;
      font-family: Arial,sans-serif;
    }
    h1 {
      text-align: center;
      font-size: 4rem;
    }
    h4 {
      font-style: italic;
    }
    .imgcontrol {
      width: 40vh;
      display: block;
      margin: auto;
    }
    .center {
      text-align: center;
    }
    a {
      color: #1976d2;
    }
    a:hover {
      color: #76C4E2;
    }
  body
    h1 #{$i18n.error.notfound.title}
    img(class='imgcontrol center' alt="Lost Image" src='data:image/svg+xml;base64,')
    h4(class='center') #{$i18n.error.notfound.desc}
    p(class='center')
      a(href='/') #{$i18n.error.notfound.link}

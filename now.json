{
  "version": 2,
  "alias": ["www.walcron.com", "walcron.com", "login.walcron.com"],
  "name": "walcron",
  "routes": [
    {"src": "^/favicon.ico$", "dest": "/static/seo/favicon.ico", "headers": {"cache-control": "max-age=86400, public"}},
    {"src": "^/robots.txt$", "dest": "/static/seo/robots.txt", "headers": {"cache-control": "max-age=86400, public"}},
    {"src": "^/og_image.png$", "dest": "/static/seo/og_image.png", "headers": {"cache-control": "max-age=86400, public"}},
    {"src": "^/sitemap.xml$", "dest": "/static/seo/sitemap.xml", "headers": {"cache-control": "max-age=86400, public"}},
    {"src": "^/static/host/(.*)/$", "dest": "/static/host/$1/index.html" },
    {"src": "^/static/(img|fonts|css|json)/(.*)$", "dest": "/static/$1/$2", "headers": {"cache-control": "max-age=86400, public"}}
  ],
  "github": {
    "enabled": true,
    "autoAlias": false
  }
}

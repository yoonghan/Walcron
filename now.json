{
  "version": 2,
  "alias": ["www.walcron.com", "walcron.com", "login.walcron.com"],
  "regions": ["sin1"],
  "routes": [
    {"src": "^/$", "headers": {"x-content-type-options": "nosniff", "x-frame-options": "SAMEORIGIN", "Content-Security-Policy": "frame-ancestors 'self'; connect-src https://*.now.sh https://www.google-analytics.com; default-src 'self'; style-src 'self' 'unsafe-inline' https://tagmanager.google.com https://fonts.googleapis.com; font-src *; img-src 'self' https://ssl.gstatic.com https://www.gstatic.com https://www.google-analytics.com data:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com https://ssl.google-analytics.com", "X-XSS-Protection": "1; mode=block", "base-uri": "none", "form-action": "https://www.googleapis.com/"}, "dest":"/"},
    {"src": "^/(img|fonts|css|json)/(.*)$", "headers": {"cache-control": "max-age=31536000 s-maxage=31536000"}, "dest": "/$1/$2", "continue": true},
    {"src": "^/host/(.*)/$", "dest": "/host/$1/index.html" },
    {"src": "^/service-worker.js$", "headers": {
      "cache-control": "public, max-age=43200, immutable",
      "Service-Worker-Allowed": "/"
      },
      "dest": "/.next/service-worker.js"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "env": {
    "PUSHER_APP_ID": "@pusher-api-id",
    "PUSHER_APP_KEY": "@pusher-app-key",
    "PUSHER_SECRET": "@pusher-secret",
    "PUSHER_CLUSTER": "@pusher-cluster",
    "PUSHER_NONAUTH_APP_ID": "@pusher-nonauth-api-id",
    "PUSHER_NONAUTH_APP_KEY": "@pusher-nonauth-app-key",
    "PUSHER_NONAUTH_SECRET": "@pusher-nonauth-secret",
    "AUTH_API_CALL": "@auth-api-call",
    "FIREBASE_BUCKET": "@firebase-bucket",
    "FIREBASE_DATABASE": "@firebase-database"
  },
  "build": {
    "env": {
      "PUSHER_APP_KEY": "@pusher-app-key",
      "PUSHER_NONAUTH_APP_KEY": "@pusher-nonauth-app-key",
      "PUSHER_CLUSTER": "@pusher-cluster"
    }
  }
}

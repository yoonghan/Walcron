{
  "version": 2,
  "alias": ["www.walcron.com", "walcron.com", "login.walcron.com"],
  "name": "Walcron",
  "regions": ["sin1"],
  "routes": [
    {"src": "^/$", "headers": {"x-content-type-options": "nosniff", "x-frame-options": "SAMEORIGIN", "Content-Security-Policy": "frame-ancestors 'self'; default-src 'self'; style-src 'self' 'unsafe-inline' https:; font-src *; img-src 'self' data:; script-src 'self' 'unsafe-inline' 'unsafe-eval'", "X-XSS-Protection": "1; mode=block"}, "dest":"/"},
    {"src": "^/static/(img|fonts|css|json)/(.*)$", "headers": {"cache-control": "s-maxage=31536000"}, "dest": "/static/$1/$2"},
    {"src": "^/static/host/(.*)/$", "dest": "/static/host/$1/index.html" },
    {"src": "^/favicon.ico$", "headers": {"cache-control": "s-maxage=31536000"}, "dest": "/static/seo/favicon.ico"},
    {"src": "^/robots.txt$", "headers": {"cache-control": "s-maxage=31536000"}, "dest": "/static/seo/robots.txt"},
    {"src": "^/og_image.png$", "headers": {"cache-control": "s-maxage=31536000"}, "dest": "/static/seo/og_image.png"},
    {"src": "^/sitemap.xml$", "headers": {"cache-control": "s-maxage=31536000"}, "dest": "/static/seo/sitemap.xml"},
    {"src": "^/service-worker.js$", "headers": {
      "cache-control": "public, max-age=43200, immutable",
      "Service-Worker-Allowed": "/"
      },
      "dest": "/_next/static/service-worker.js"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "env": {
    "PUSHER_APP_ID": "@pusher-api-id",
    "PUSHER_APP_KEY": "@pusher-app-key",
    "PUSHER_SECRET": "@pusher-secret",
    "PUSHER_CLUSTER": "@pusher-cluster",
    "AUTH_API_CALL": "@auth-api-call"
  },
  "build": {
    "env": {
      "PUSHER_APP_KEY": "@pusher-app-key",
      "PUSHER_CLUSTER": "@pusher-cluster"
    }
  }
}
